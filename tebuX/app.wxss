/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.w-25{
  width: 25%;
}
.w-50{
  width: 50%;
}
.w-75{
  width: 75%;
}
.w-100{
  width: 100%;
}
.h-25{
  height: 25%;
}
.h-50{
  height: 50%;
}
.h-75{
  height: 75%;
}
.h-100{
  height: 100%;
}
.pt-1{
  padding-top: 10rpx;
}
.jt{
  width: 20rpx;
  height:20rpx;
  border-right: 5rpx solid #e8e8e8;
  border-top: 5rpx solid #e8e8e8;
  margin: auto 0;
  transform: rotate(45deg)
}
.card{
  border-bottom: 3rpx solid #ddd;
  width: 100%;
  height: 200rpx;
  position: relative;
}
.all{
  height: 100%;
  display: flex;
  padding: 10rpx;
}
.card .all image{
  margin-left: 10rpx;
  width: 180rpx;
  height: 180rpx;
}
.red{
  color: #e60012
}
