page{
  padding-bottom:  124rpx;
}
swiper,swiper image{
  width: 100%;
  height: 750rpx;
}
.title{
  font-size: 15px;
  padding: 30rpx;
  font-weight: bold;
  border-top: 1rpx solid #e8e8e8;
  border-bottom: 1rpx solid #e8e8e8;
}
.price{
  padding-left: 30rpx;
  padding-right: 30rpx;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  font-size: 12px;
  display: flex;
  justify-content: space-between;
}
.price>view:first-child text{
  text-decoration:line-through;
}
.price>view:last-child text{
  color: red;
  font-weight: bold;
}
.prices{
  display:flex;
  justify-content: space-between;
  color: #fff;
  background: url(http://172.20.10.6:1674/img/shopinfo/msBg.png) no-repeat, linear-gradient(rgb(252, 154, 154), rgb(230, 27, 37));
  padding: 30rpx;
  font-size: 15px;
}
.prices text{
  font-weight: bold;
}
.time{
  font-size: 13px;
  color: #8f8f94;
  padding: 30rpx;
}
.time view:first-child{
  padding-bottom: 15rpx;
}
.radio{
  display: flex;
  justify-content: space-between;
  font-size: 13px;
  padding: 30rpx;
}
.shoplist{
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  transition: all 0.5s ;
  z-index: 99;
}
.shoplistClose{
  position: fixed;
  bottom: -826rpx;
  left: 0;
}
.shop{
  position:fixed;
  top: 0;
  left:0;
  background: #000;
  opacity: 0.5;
  width: 100%;
  height: 100%;
  z-index: 10;
}
radio{
  font-size: 13px;
}
.shoplist image{
  width: 350rpx;
  height: 350rpx;
  padding: 30rpx;
}
.shoplist>button{
  width: 100%;
  background: #ef4f4f;
  color: #fff;
}
.shoplist>view{
  background: #fff;
}
.radios{
  display: flex;
  justify-content: space-between;
}
radio-group{
  height: 324rpx;
  overflow: auto;
  width: 100%;
}
radio-group view{
  padding: 25rpx;
}
.prop{
  display: flex;
  justify-content: space-between;
}
.prop>view{
  display: flex;
  flex-flow: column;
  justify-content: space-between;
}
.close{
  font-size: 20px;
  text-align: right;
  padding: 20rpx;
}
.prop>.w-50>view:nth-child(2){
  overflow:hidden;
  text-overflow:ellipsis;
  display:-webkit-box;
  -webkit-box-orient:vertical;
  -webkit-line-clamp:2;
  font-size: 15px;
  font-weight: bold;
}
.prop>.w-50>view:nth-child(3){
  margin-top:5rpx;
  font-size: 17px;
  font-weight: bold;
  color: #ef4f4f;
}
.prop>.w-50>view:nth-child(4){
  margin: 5rpx;
  display: flex;
}
.prop>.w-50>view:nth-child(4)>button,.prop>.w-50>view:nth-child(4)>text{
  width: 100%;
  text-align: center;
  line-height: 92rpx;
}
.footer{
  display: flex;
  text-align: center;
  font-size: 13px;
  color: #fff;
  position: fixed;
  bottom: 0;
}
.footer>view:first-child,.footer>view:nth-child(2){
  background: #000;
  display: flex;
  justify-content: center;
  flex-flow: column;
  border-right: 4rpx solid #fff;
  height: 124rpx; 
}
.footer>view:first-child>image,.footer>view:nth-child(2)>image{
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}
.footer>view:last-child{
  line-height: 124rpx;
  background: rgb(255, 102, 0);
  font-weight: bold;
  font-size: 18px;
}
.inCo{
  position: relative;
  border-top: 5rpx solid #e8e8e8;
  display: flex;
  text-align: center; 
  padding: 20rpx 0;
  transition:all 0.5s ease-in-out;
}
.inCo>.w-50{
  position: absolute;
  left: 0;
  top:85rpx;
  height: 5rpx;
  background: #ef4f4f;
  transition:all 0.5s ease-in-out;
}
.inCoActive{
  left: 50% !important;
}
.ac_color{
  color: #ef4f4f;
  transition:all 0.5s ease-in-out;
}
.infos{
  padding: 30rpx;
  transition:all 0.5s ease-in-out;
  overflow: hidden;
}
.infos>view:first-child .w-25{
  display: flex;
  justify-content: space-between;
  font-weight: bold;
}
.infos>view:first-child>view{
  display: flex;
  font-size: 16px;
  padding-bottom: 20rpx;
}
.infos>view:first-child>view>view:nth-child(2){
  white-space: pre-wrap;
  width: 75%;
}
.bimg{
  padding: 30rpx;
}
.bimg image{
  width: 100%;
}
.bud{
  font-size: 15px;
  color:rgb(128, 121, 121);
  margin-right: 20rpx;
}
.ctime{
  color: #ccc;
  font-size: 12px;
}
.name{
  font-size: 15px;
}
.cart{
  position: fixed;
  top: 40%;
  right: 20rpx;
  opacity: 0.8;
  background: #eee;
  text-align: center;
  font-size: 10px;
  border-radius: 50%;
  padding: 30rpx;
}
.cart image{
  width: 50rpx;
  height: 50rpx;
}