.bg1{
  width: 100%;
  height: 350rpx;
  background:url(http://172.20.10.6:1674/img/admin/my_bg.jpg) no-repeat;
  background-size:100% 100%;
  position: relative;
}
.bg1 image{
  position: absolute;
  left: 0;
  right: 0;
  top: 45rpx;
  margin: auto;
  width: 140rpx;
  border-radius: 50%;
}
.bg1>.a1{
  background: #000;
  opacity: 0.5;
  position: absolute;
  bottom: 0;
  width: 100%;
  height: 120rpx;
}
.bg1>view+view{
  display: flex;
  position: absolute;
  bottom: 0;
  width: 100%;
  color: #fff;
  font-weight: bold;
}
.bg1>view>view{
  text-align: center;
  font-size: 14px;
  padding: 40rpx 0;
}
.list{
  display: flex;
  padding: 25rpx;
  justify-content: space-between;
  border-bottom: 1rpx solid #ddd;
  font-size: 20px;
}
.iconfont{
  margin-right: 30rpx;
}